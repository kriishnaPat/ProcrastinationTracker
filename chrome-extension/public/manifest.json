{
  "name": "Procrastination Tracker",
  "description": "Extension that tracks your procrastination and provides detail data abouts your browsing habits",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "scripting", "alarms", "tabs", "activeTab", "background", "history"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/logo.png",
      "32": "/images/logo.png",
      "48": "/images/logo.png",
      "128": "/images/logo.png"
    }
  },
  "icons": {
    "16": "/images/logo.png",
    "32": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  }
}