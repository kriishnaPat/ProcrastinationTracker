{
  "name": "Procrastination Tracker",
  "description": "Extension that tracks your procrastination and provides detail data abouts your browsing habits",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background-wrapper.js"
  },
  "permissions": ["storage", "scripting", "alarms", "tabs","activeTab","background"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}